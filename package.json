{
  "publisher": "ririd",
  "name": "vscode-ext-iconfont-reminder",
  "displayName": "iconfont-reminder",
  "version": "0.1.8",
  "description": "字体图标预览",
  "license": "MIT",
  "homepage": "https://github.com/Daydreamer-riri/vscode-ext-iconfont-reminder",
  "repository": {
    "type": "git",
    "directory": "https://github.com/Daydreamer-riri/vscode-ext-iconfont-reminder"
  },
  "keywords": [
    "icons",
    "iconfont"
  ],
  "categories": [
    "Other",
    "Visualization",
    "Snippets"
  ],
  "main": "./dist/extension.js",
  "icon": "res/logo.png",
  "engines": {
    "vscode": "^1.57.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "iconfont-reminder configuration",
      "properties": {
        "iconfontReminder.mapFilePath": {
          "scope": "resource",
          "description": "File describing the iconfont mapping",
          "type": "string"
        },
        "iconfontReminder.svgPath": {
          "scope": "resource",
          "description": "Svg file",
          "type": "string"
        },
        "iconfontReminder.componentName": {
          "scope": "resource",
          "description": "Component name",
          "type": "string",
          "default": "Icon"
        }
      }
    }
  },
  "scripts": {
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "release": "bumpp --tag --push --commit",
    "dev": "pnpm build --watch",
    "build": "esno scripts/build.ts"
  },
  "dependencies": {
    "jiti": "^1.18.2"
  },
  "devDependencies": {
    "@ririd/eslint-config": "^0.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "~16.18.18",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "bumpp": "^9.0.0",
    "eslint": "^8.36.0",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.1",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5"
  }
}

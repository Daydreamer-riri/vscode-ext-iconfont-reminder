{
  "publisher": "ririd",
  "name": "vscode-ext-iconfont-reminder",
  "displayName": "iconfont-reminder",
  "version": "0.0.1",
  "description": "字体图标预览",
  "homepage": "https://github.com/Daydreamer-riri/vscode-ext-iconfont-reminder",
  "repository": {
    "type": "git",
    "directory": "https://github.com/Daydreamer-riri/vscode-ext-iconfont-reminder"
  },
  "keywords": [
    "icons",
    "iconfont"
  ],
  "categories": [
    "Other",
    "Visualization",
    "Snippets"
  ],
  "main": "./dist/extension.js",
  "engines": {
    "vscode": "^1.47.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "iconfont-reminder configuration",
      "properties": {
        "iconfontReminder.mapFilePath": {
          "scope": "resource",
          "description": "File describing the iconfont mapping",
          "type": "string"
        },
        "iconfontReminder.svgPath": {
          "scope": "resource",
          "description": "Svg file",
          "type": "string"
        },
        "iconfontReminder.componentName": {
          "scope": "resource",
          "description": "Component name",
          "type": "string",
          "default": "Icon"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@ririd/eslint-config": "^0.1.3",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "~16.18.11",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  }
}
